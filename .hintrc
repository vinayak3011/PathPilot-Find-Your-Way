{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-decoration-skip"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 115",
    "not firefox <= 116",
    "not safari <= 16.5"
  ]
}